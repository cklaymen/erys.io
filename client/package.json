{
  "name": "main",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.47",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-i18next": "^8.1.0",
    "@types/styled-components": "^5.1.0",
    "graphql": "^15.3.0",
    "i18next": "^19.5.4",
    "marked": "^1.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-i18next": "^11.7.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.1.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "npm run graphql-codegen && react-scripts start",
    "build": "npm run graphql-codegen && react-scripts build",
    "postbuild": "react-snap && npm run generate-sitemap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "graphql-codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "generate-sitemap": "ts-node --project ./generate-sitemap/tsconfig.json ./generate-sitemap/index.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >= 9"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/marked": "^1.1.0",
    "@types/react-router-dom": "^5.1.5",
    "react-snap": "../react-snap",
    "ts-node": "^9.0.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "skipThirdPartyRequests": true,
    "api": "http://localhost:1337"
  }
}
